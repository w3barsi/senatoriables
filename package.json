{
  "name": "senatoriables",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "start:xargs": "export $(grep -v '^#' .env | xargs) && pnpm start",
    "start:node": "node -r dotenv/config .output/server/index.mjs",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db": "drizzle-kit",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "ui": "pnpm dlx shadcn@canary",
    "auth": "pnpm dlx @better-auth/cli@latest",
    "auth:generate": "pnpm dlx @better-auth/cli@latest generate --y --output ./src/lib/server/schema/auth.schema.ts && prettier --write ./src/lib/server/schema/auth.schema.ts",
    "deps": "pnpm dlx npm-check-updates@latest --interactive --format group",
    "flush": "rm -rf .output .vinxi node_modules && pnpm install"
  },
  "dependencies": {
    "@libsql/client": "^0.15.4",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-label": "^2.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@tanstack/react-query": "^5.74.3",
    "@tanstack/react-query-devtools": "^5.74.3",
    "@tanstack/react-router": "^1.116.0",
    "@tanstack/react-router-devtools": "^1.116.0",
    "@tanstack/react-router-with-query": "^1.116.0",
    "@tanstack/react-start": "^1.116.1",
    "@vitejs/plugin-react": "^4.4.0",
    "better-auth": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.42.0",
    "lucide-react": "^0.488.0",
    "nanoid": "^5.1.5",
    "postgres": "^3.4.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "vinxi": "^0.5.4"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.47.4",
    "@eslint/js": "^9.24.0",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/eslint-plugin-query": "^5.73.3",
    "@tanstack/eslint-plugin-router": "^1.115.0",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "babel-plugin-react-compiler": "latest",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-react-compiler": "latest",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
